                },
                "createdAt": "Recept created time",
                "updatedAt": "Recept updated time",
                "ingredients": {
                  "type": [
                    {
                      "ingredientId": {
                        "type": "Schema.Types.ObjectId",
                        "description": "Ingredient's id"
                      },
                      "measure": {
                        "type": "String",
                        "description": "Ingredient's measure ibn recipe"
                      }
                    }
                  ]
                }
              }
            }
          }
        },
