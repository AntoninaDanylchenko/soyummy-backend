{
  "openapi": "3.0.0",
  "info": {
    "version": "1.0.0",
    "title": "First So Yummy Team`s backend",
    "description": "Showing off swagger-ui-express",
    "license": {
      "name": "MIT",
      "url": "https://opensource.org/licenses/MIT"
    }
  },
  "consumes": ["application/json", "multipart/form-data"],
  "produces": ["application/json"],
  "servers": [{ "url": "https://soyummy-backend-first-team.onrender.com" }],
  "tags": [
    {
      "name": "Auth",
      "description": "Authorization endpoints"
    },
    {
      "name": "Subscribe",
      "description": "Subscribtion email"
    },
    {
      "name": "Recipes",
      "description": "Everything about Recipes"
    },
    {
      "name": "Ingredients",
      "description": "Everything about Ingredients"
    },
    {
      "name": "Favorite recipes",
      "description": "Find, remove and add favorite recipes"
    },
    {
      "name": "Popular recipes",
      "description": "Find popular recipes"
    },
    {
      "name": "Own recipe",
      "description": "Everything about your recipes"
    },
    {
      "name": "Shopping list",
      "description": "Everything about your shopping list"
    },
    {
      "name": "Modal",
      "description": "Find information about user"
    }
  ],
  "paths": {
    "/auth/signup": {
      "post": {
        "tags": ["Auth"],
        "summary": "User registration",
        "parameters": [],
        "requestBody": {
          "description": "Registration's object",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/RegistrationRequest"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/RegistrationResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad request (invalid request body)",
            "content": {}
          },
          "409": {
            "description": "Email should be unique",
            "content": {}
          }
        }
      }
    },
    "/auth/login": {
      "post": {
        "tags": ["Auth"],
        "summary": "User login",
        "parameters": [],
        "security": [{ "Bearer": [] }],
        "requestBody": {
          "description": "Login object",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/LoginResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad request (invalid request body)",
            "content": {}
          },
          "401": {
            "description": " Email or password is incorrect",
            "content": {}
          },
          "409": {
            "description": "Provided email already exists",
            "content": {}
          }
        }
      }
    },
    "/auth/current": {
      "get": {
        "tags": ["Auth"],
        "summary": "Return current user",
        "parameters": [],
        "security": [{ "Bearer": [] }],

        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/LoginResponse"
                }
              }
            }
          },
          "401": {
            "description": " Token expired, please Login again.",
            "content": {}
          }
        }
      }
    },
    "/auth/logout": {
      "post": {
        "tags": ["Auth"],
        "summary": "Users` logout",
        "parameters": [],
        "security": [{ "Bearer": [] }],
        "responses": {
          "204": {
            "description": "Successful operation",
            "content": "No Content"
          },
          "401": {
            "description": "Not authorized",
            "content": {}
          }
        }
      }
    },

    "/recipes/category-list": {
      "get": {
        "tags": ["Recipes"],

        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/CategoryListResponse"
                }
              }
            }
          },

          "404": {
            "description": "Not found",
            "content": {}
          }
        }
      }
    },
    "/recipes/main-page": {
      "get": {
        "tags": ["Recipes"],
        "summary": "Find 4 recipes in 4 categories (Breakfast, Miscellaneous, Chicken, Dessert)",
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/RecipeResponse"
                }
              }
            }
          },

          "404": {
            "description": "Recipe not found",
            "content": {}
          }
        }
      }
    },
    "/recipes/{category}": {
      "get": {
        "tags": ["Recipes"],
        "summary": "Receiving 8 recipes of the selected category",
        "parameters": [
          {
            "name": "category",
            "in": "path",
            "required": true,
            "type": "string",
            "description": "Name of category",
            "example": "Beef"
          },
          {
            "name": "limit",
            "in": "query",
            "required": false,
            "type": "string",
            "description": "Amount recipes on page"
          },
          {
            "name": "page",
            "in": "query",
            "required": false,
            "type": "string",
            "description": "Number of page"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/CategoryResponse"
                }
              }
            }
          },

          "404": {
            "description": "Not found",
            "content": {}
          }
        }
      }
    },
    "/recipes/id/{id}": {
      "get": {
        "tags": ["Recipes"],
        "summary": "Find recipe by ID",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string",
            "description": "Id of recipe",
            "example": "640cd5ac2d9fecf12e889864"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/RecipeResponse"
                }
              }
            }
          },

          "400": {
            "description": "Invalid recipe id",
            "content": {}
          },

          "404": {
            "description": "Recipe not found",
            "content": {}
          }
        }
      }
    },
    "/ingredients/list": {
      "get": {
        "tags": ["Ingredients"],
        "summary": "Find list of ingredients",
        "parameters": [],
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/IngredientResponse"
                }
              }
            }
          },
          "404": {
            "description": "Not found ingredients list",
            "content": {}
          }
        }
      }
    },
    "/ingredients": {
      "get": {
        "tags": ["Ingredients"],
        "summary": "Find recipes for search ingredient",
        "parameters": [
          {
            "name": "search",
            "in": "query",
            "required": true,
            "type": "string",
            "example": "Challots"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/RecipeResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad request ingredient",
            "content": {}
          },
          "404": {
            "description": "Not found ingredient or not found recipe",
            "content": {}
          }
        }
      }
    },
    "/favorite": {
      "post": {
        "tags": ["Favorite recipes"],
        "summary": "Add recipe to favorite",
        "parameters": [],
        "security": [{ "Bearer": [] }],
        "requestBody": {
          "description": "Id of recipe",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/FavoriteResponse"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Successful operation",
            "content": "Added to favorite"
          }
        }
      },
      "get": {
        "tags": ["Favorite recipes"],
        "summary": "Get favorite recipes",
        "parameters": [],
        "security": [{ "Bearer": [] }],
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/RecipeResponse"
                }
              }
            }
          }
        }
      },
      "delete": {
        "tags": ["Favorite recipes"],
        "summary": "Delete recipe from favorite",
        "parameters": [],
        "security": [{ "Bearer": [] }],
        "requestBody": {
          "description": "Id of recipe",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/FavoriteRequest"
              }
            }
          }
        },
        "responses": {
          "204": {
            "description": "Successful operation"
          }
        }
      }
    },
    "/popular-recipe": {
      "get": {
        "tags": ["Popular recipes"],
        "summary": "Find recipes for search ingredient",
        "parameters": [],
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ModalResponse"
                }
              }
            }
          },
          "404": {
            "description": "Not found popular recipes",
            "content": {}
          }
        }
      }
    },
    "/search": {
      "get": {
        "tags": ["Recipes"],
        "summary": "Find recipes for title",
        "parameters": [
          {
            "name": "search",
            "in": "query",
            "required": true,
            "type": "string",
            "example": "Beef"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/RecipeResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad request",
            "content": {}
          },
          "404": {
            "description": "Not found recipe",
            "content": {}
          }
        }
      }
    },
    "/modal": {
      "get": {
        "tags": ["Modal"],
        "summary": "Find information about user, how days he is loged, how many has own recupes and how many recipes adds to favorite",
        "parameters": [],
        "security": [{ "Bearer": [] }],
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ModalResponse"
                }
              }
            }
          },
          "401": {
            "description": "Not authorized, or Token expired, please Login again, or Invalid token, or Email or password is incorrect",
            "content": {}
          }
        }
      }
    },
    "/shopping-list": {
      "get": {
        "tags": ["Shopping list"],
        "summary": "Find all ingredients from shopping list",
        "parameters": [],
        "security": [{ "Bearer": [] }],
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GetShoppingListResponse"
                }
              }
            }
          }
        }
      },
      "post": {
        "tags": ["Shopping list"],
        "summary": "Add ingredient to shopping list",
        "parameters": [],
        "security": [{ "Bearer": [] }],
        "requestBody": {
          "description": "Ingredient object",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ShoppingListRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PostShoppingListResponse"
                }
              }
            }
          }
        }
      },
      "delete": {
        "tags": ["Shopping list"],
        "summary": "Delete ingredient from shopping list",
        "parameters": [],
        "security": [{ "Bearer": [] }],
        "requestBody": {
          "description": "Ingredient object",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ShoppingListIdRequest"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PostShoppingListResponse"
                }
              }
            }
          },
          "404": {
            "description": "Not found",
            "content": {}
          }
        }
      }
    },
    "/subscribe": {
      "get": {
        "tags": ["Subscribe"],
        "summary": "Send hello mail",
        "parameters": [],
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {}
          }
        }
      },
      "post": {
        "tags": ["Shopping list"],
        "summary": "Add ingredient to shopping list",
        "parameters": [],
        "security": [{ "Bearer": [] }],
        "requestBody": {
          "description": "Ingredient object",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ShoppingListRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PostShoppingListResponse"
                }
              }
            }
          }
        }
      },
      "delete": {
        "tags": ["Shopping list"],
        "summary": "Delete ingredient from shopping list",
        "parameters": [],
        "security": [{ "Bearer": [] }],
        "requestBody": {
          "description": "Ingredient object",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ShoppingListIdRequest"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PostShoppingListResponse"
