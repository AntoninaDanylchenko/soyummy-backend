              }
            }
          },
          "404": {
            "description": "Not found",
            "content": {}
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "RegistrationRequest": {
        "type": "object",
        "required": ["email", "username", "password"],
        "properties": {
          "email": {
            "type": "string",
            "description": "User's email",
            "format": "email"
          },
          "username": {
            "type": "string",
            "description": "User's name",
            "example": "Andrew"
          },
          "password": {
            "type": "string",
            "description": "User's password",
            "example": "qwerty123"
          }
        }
      },
      "RegistrationResponse": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "userId": {
              "type": "string",
              "description": "User's id",
              "example": "32143232436545474"
            },
            "email": {
              "type": "string",
              "description": "User's email",
              "format": "email"
            },
            "username": {
              "type": "string",
              "description": "User's name",
              "example": "Andrew"
            },
            "avatarURL": {
              "type": "string",
              "description": "Link to user`s avatar url",
              "example": "//www.gravatar.com/b58996c504c5638798eb6b511e6f49af"
            }
          },
          "example": [
            {
              "resUser": {
                "_id": "6443d47d40e926b3c1d9755c",
                "username": "Andrew",
                "email": "user@example.com",
                "avatarURL": "//www.gravatar.com/avatar/b58996c504c5638798eb6b511e6f49af"
              }
            }
          ]
        }
      },
      "LoginRequest": {
        "type": "object",
        "required": ["email", "password"],
        "properties": {
          "email": {
            "type": "string",
            "description": "User's email",
            "format": "email"
          },
          "password": {
            "type": "string",
            "description": "User's password",
            "example": "qwerty123"
          }
        }
      },
      "LoginResponse": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "resUser": {
              "type": "object",
              "properties": {
                "_id": {
                  "type": "number",
                  "description": "User's id",
                  "example": "32143232436545474"
                },
                "email": {
                  "type": "string",
                  "description": "User's email",
                  "format": "email"
                },
                "username": {
                  "type": "string",
                  "description": "User's name",
                  "example": "Andrew"
                },
                "avatarURL": {
                  "type": "string",
                  "description": "Link to user`s avatar url",
                  "example": "//www.gravatar.com/b58996c504c5638798eb6b511e6f49af"
                }
              }
            },

            "token": {
              "type": "number",
              "example": "32143232436545474"
            }
          },
          "example": {
            "resUser": {
              "_id": "6443e37ad6feaf6e0cbafd41",
              "username": "Andrew",
              "email": "user2@example.com",
              "avatarURL": "//www.gravatar.com/avatar/ab53a2911ddf9b4817ac01ddcd3d975f"
            },
            "token": "eyJhbGcNiIsInR5.eyJpZCI6IjYhZDmUwY2JhZmQIjoidXZXhhbXBsZS5jb20iLCJpYXQiOjE2ODIxODg0NzAsImV4cCI6MTY4MjI3MTI3MH0.mM8DVmEcs_UOeEu_DKTPtWyvMEnDhT3oXbIx6A1PbBY"
          }
        }
      },
      "CategoryListResponse": {
        "type": "array",
        "items": {
          "type": "string",
          "properties": {
            "description": "Category`s name"
          },
          "example": ["Beef", "Vegan"]
        }
      },
      "CategoryResponse": {
        "type": "object",
        "items": {
          "type": "string",
          "properties": {
            "total": {
              "type": "string",
              "description": "Count of recepes in category",
              "example": "45"
            },
            "recipesList": {
              "type": "object",
              "properties": {
                "title": {
                  "type": "String",
                  "description": "Recipe title"
                },
                "category": {
                  "type": "String",
                  "description": "Recipe category"
                },
                "area": {
                  "type": "String",
                  "description": "Recipe country"
                },
                "instructions": {
                  "type": "String",
                  "description": "Instruction of recipe"
                },
                "description": {
                  "type": "String",
                  "description": "Recipe description"
                },
                "thumb": {
                  "type": "String",
                  "description": "Recipe photo"
                },
                "preview": {
                  "type": "String",
                  "description": "Recipe photo preview"
                },
                "time": {
                  "type": "String",
                  "description": "Recipe preparation time"
                },
                "youtube": {
                  "type": "String",
